html
  head
    title= title
  body
    h1= message
    div.userControls
      
    div.container
      h3= "Files"
      each val in files
        a(href='/view/?file='+val["file_id"]) #{val["file_name"]}
      else
        p No values
      h3= "Friends"
      each friend in friends
        a(href="/chat/?user1="+user+"&"+"user2="+friend) #{friend}
      else
        p No friends
